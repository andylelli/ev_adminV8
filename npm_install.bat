call npm i @vitejs/plugin-vue

REM FONT AWESOME
call npm i --save @fortawesome/fontawesome-svg-core
call npm i --save @fortawesome/pro-light-svg-icons
call npm i --save @fortawesome/vue-fontawesome@latest-3
call npm i --save @fortawesome/pro-regular-svg-icons
